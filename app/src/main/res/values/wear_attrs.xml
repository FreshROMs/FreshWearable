<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="WearDashboardShortcutItem">
        <attr name="wShortcutIcon" format="reference" />
        <attr name="wShortcutName" format="string" />
        <attr name="wShortcutActivity" format="string" />
    </declare-styleable>

    <declare-styleable name="NoiseControlPreference">
        <attr name="ancValue" format="string" />
        <attr name="offValue" format="string" />
        <attr name="transparencyValue" format="string" />
        <attr name="selectedValue" format="string" />
    </declare-styleable>

    <declare-styleable name="ButtonGroupPreference">
        <attr name="entries" format="reference" /> <!-- Array of display names -->
        <attr name="entryValues" format="reference" /> <!-- Array of corresponding values -->
    </declare-styleable>

    <declare-styleable name="EqualizerPreviewPreference">
        <attr name="drawableEntries" format="reference" /> <!-- Array of drawable names -->
        <attr name="graphEntries" format="reference" /> <!-- Array of corresponding values -->
    </declare-styleable>

    <declare-styleable name="WidgetPreviewItem">
        <attr name="wpTitle" format="string" />
        <attr name="wpSubtitle" format="string" />
    </declare-styleable>
</resources>