<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HorizontalProgressView">
        <attr name="hProgress" format="float" />
        <attr name="hMin" format="float" />
        <attr name="hMax" format="float" />
        <attr name="hBackgroundColor" format="color" />
        <attr name="hProgressColor" format="color" />
        <attr name="hDotColor" format="color" />
        <attr name="hDotBorderColor" format="color" />
    </declare-styleable>

    <declare-styleable name="CircularProgressView">
        <attr name="cProgress" format="float" />
        <attr name="cMax" format="float" />
        <attr name="cStroke" format="dimension" />
        <attr name="cBackgroundColor" format="color" />
        <attr name="cProgressColor" format="color" />
    </declare-styleable>
</resources>